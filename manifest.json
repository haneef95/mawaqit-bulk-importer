{
  "manifest_version": 3,
  "name": "Mawaqit Bulk Calendar Importer",
  "version": "2.5.9",
  "description": "Bulk import prayer times from CSV files into Mawaqit calendar",
  "author": "You",
  "permissions": [],
  "host_permissions": [
    "*://mawaqit.net/*/configure*",
    "*://*.mawaqit.net/*/configure*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://mawaqit.net/*/configure*",
        "*://*.mawaqit.net/*/configure*"
      ],
      "js": ["timeConverter.js", "dstConverter.js", "content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon.svg",
    "48": "icons/icon.svg",
    "128": "icons/icon.svg"
  }
}
